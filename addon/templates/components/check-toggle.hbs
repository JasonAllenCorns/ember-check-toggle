
<span class="labelText">{{{yield}}}</span>
<span class="toggle-track">
    {{input type="checkbox" name=toggleName class="toggle-input" checked=isChecked}}
    <span class="toggle-label-before">{{open-text}}</span>
    <span class="toggle-label"></span>
    <span class="toggle-label-after">{{checked-text}}</span>
    <span class="toggle-handle"></span>
</span>
